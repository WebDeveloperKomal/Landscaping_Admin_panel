{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/forms": "off",
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-webkit-tap-highlight-color",
          "text-decoration-skip-ink",
          "text-align: -webkit-match-parent",
          "-webkit-overflow-scrolling",
          "accent-color",
          "overflow-anchor",
          "text-underline-offset",
          "scrollbar-color",
          "scrollbar-color",
          "scrollbar-width",
          "scrollbar-width"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 114",
    "not chrome <= 114",
    "not edge <= 114",
    "not ios_saf <= 16.5",
    "not opera <= 100",
    "not safari <= 16.5",
    "not samsung <= 21"
  ]
}